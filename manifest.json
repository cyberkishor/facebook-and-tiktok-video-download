{
  "name": "FB  & TicTok Download",
  "version": "1.0.1",
  "description": "Download Ad Videos from Facebook Timeline and Tiktok",
  "manifest_version": 3,
  "author": "Deelip Khanal",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/ads/library/*",
        "https://www.facebook.com/*",
        "https://www.tiktok.com/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/img/download-icon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}